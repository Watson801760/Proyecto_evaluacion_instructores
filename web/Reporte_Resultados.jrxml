	<queryString language="SQL">
		<![CDATA[SELECT
     evalua.`respuesta` AS evalua_respuesta,
     funcionario.`nombre` AS funcionario_nombre,
     funcionario.`apellido` AS funcionario_apellido,
     funcionario.`correo` AS funcionario_correo,
     funcionario.`numIdentidad` AS funcionario_numIdentidad,
     aprendiz.`nombre` AS aprendiz_nombre,
     aprendiz.`apellido` AS aprendiz_apellido,
     aprendiz.`correo` AS aprendiz_correo,
     aprendiz.`numIdentidad` AS aprendiz_numIdentidad,
     pregunta.`pregunta` AS pregunta_pregunta
FROM
     `funcionario` funcionario INNER JOIN `evalua` evalua ON funcionario.`idFuncionario` = evalua.`idFuncionario(FK)`
     INNER JOIN `aprendiz` aprendiz ON evalua.`idAprendiz(FK)` = aprendiz.`idAprendiz`
     INNER JOIN `pregunta` pregunta ON evalua.`idPregenta(FK)` = pregunta.`idPregunta`]]>
	</queryString>
